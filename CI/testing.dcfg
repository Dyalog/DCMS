{
	Settings: {
		MAXWS:         "2G",

		ADMIN_DIR:     "[APP_DIR]/Admin",
		DCMS_DIR:      "[APP_DIR]/APLSource",
		SECRETS:       "[APP_DIR]/secrets/secrets.json5",
		URL:           "[SERVICE_URL]:[SERVICE_PORT]",

		MOCKYT:        "http://localhost:8088/",

		Jarvis:        "[APP_DIR]/packages/Jarvis.dyalog",
		HttpCommand:   "[APP_DIR]/packages/HttpCommand.dyalog",

		LX:            "⎕←⎕SE.Link.Import 'Admin',⍥⊂ '[ADMIN_DIR]' ⋄\
		                ⎕←⎕SE.Link.Import 'DCMS' ,⍥⊂ '[DCMS_DIR]' ⋄\
		                ⎕←⎕SE.Link.Import ⎕THIS '[HttpCommand]' ⋄\
						result←Admin.Tests.Run Admin.GetEnv'URL' ⋄\
						:If 1≡result ⋄ ⎕←'All tests passed.',⎕UCS 10 ⋄ ⎕OFF 0 ⋄ :Else ⋄ ⎕OFF 1 ⋄ :EndIf"
	}
}
